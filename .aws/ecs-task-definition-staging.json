{
    "family": "ec-private-gpt-staging-task-definition",
    "taskRoleArn": "arn:aws:iam::363611146132:role/ec-private-gpt-staging-task-role",
    "executionRoleArn": "arn:aws:iam::363611146132:role/ec-private-gpt-staging-task-role",
    "containerDefinitions": [
        {
            "name": "ec-private-gpt",
            "image": "363611146132.dkr.ecr.us-east-1.amazonaws.com/ec-private-gpt:1e2b775b2089613e0d3fdbf18b016eb112042cb2",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "ec-private-gpt-8080-http",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "healthCheck": {
                "retries": 3,
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/health || exit 1"
                ],
                "timeout": 5,
                "interval": 30,
                "startPeriod": null
            },
            "secrets": [
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:363611146132:secret:/ec-private-gpt/staging-fjZ8Mt:OPENAI_API_KEY::"
                },
                {
                    "name": "QDRANT_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:363611146132:secret:/ec-private-gpt/staging-fjZ8Mt:QDRANT_API_KEY::"
                },
                {
                    "name": "QDRANT_URL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:363611146132:secret:/ec-private-gpt/staging-fjZ8Mt:QDRANT_URL::"
                },
                {
                    "name": "PGPT_AUTH_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:363611146132:secret:/ec-private-gpt/staging-fjZ8Mt:PGPT_AUTH_SECRET::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ec-private-gpt-staging-task-definition",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "networkMode": "awsvpc",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
